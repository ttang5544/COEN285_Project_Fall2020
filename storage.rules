rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // match /{allPaths=**} {
    //   allow read, write: if request.auth!=null;
    // }
    match /users/{uid} {
      // any logged in user can read
      allow read: if request.auth != null && request.auth.uid == $uid;
    }
  }
}
